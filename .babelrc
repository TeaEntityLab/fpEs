{
  "presets": [
    "stage-0",
    "stage-1",
    "stage-2",
    "stage-3",
    "latest",
    ["env", {
      "targets": {
        "node": "current"
      }
    }]
  ],
  "plugins": [
    "transform-object-rest-spread",
    "syntax-dynamic-import",
    "transform-es2015-modules-commonjs", "dynamic-import-node",
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }]
  ]
}
