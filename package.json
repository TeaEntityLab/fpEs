{
  "name": "fpes",
  "version": "1.0.3",
  "description": "Functional Programming for EcmaScript(Javascript)",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require should test/**/*.js",
    "build": "babel src -d ./dist",
    "clean": "find src -name '*.js' | sed 's/^src\\///' | xargs rm -f",
    "prepublish": "npm run build",
    "postpublish": "npm run clean"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TeaEntityLab/fpEs.git"
  },
  "keywords": [
    "functional-programming",
    "fp",
    "js",
    "es",
    "es6",
    "es7",
    "javascript"
  ],
  "author": "John Lee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TeaEntityLab/fpEs/issues"
  },
  "homepage": "https://github.com/TeaEntityLab/fpEs#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "mocha": "^5.2.0",
    "should": "^13.2.1"
  },
