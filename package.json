    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TeaEntityLab/fpEs.git"
  },
  "keywords": [
    "functional-programming",
    "fp",
    "js",
    "es",
    "es6",
    "es7",
    "javascript"
  ],
  "author": "John Lee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TeaEntityLab/fpEs/issues"
  },
  "homepage": "https://github.com/TeaEntityLab/fpEs#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "mocha": "^5.2.0",
    "should": "^13.2.1"
  }
}
